import math
t = str (input())
if t =='треугольник':
    a,b,c = float (input()),float (input()),float (input())
    p=((a+b+c)/2)
    print (math.sqrt(p*(p-a)*(p-b)*(p-c)))
elif t =='прямоугольник':
    a,b = float (input()),float (input())
    print (a * b)
elif t =='круг':
    r = float (input())
    print (3.14*(r**2))
